{
  "name": "bacs-file-data-generator",
  "version": "0.0.0-dev",
  "description": "Domain library for generating BACS style test data (EaziPay, SDDirect, Bacs18PaymentLines)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.0",
    "luxon": "^3.5.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "vitest": "^1.6.0",
    "@types/node": "^20.11.0",
    "@types/luxon": "^3.3.0",
  "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0"
  }
}